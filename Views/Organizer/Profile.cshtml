
@model EventApp.DTOs.OrganizerDto
@{
    Layout = "_OrganizerDashBoard";
}


<dl class="row">
    <dt class="col-sm-2">
        FirstName:
    </dt>
    <dd class="col-sm-10">
        @Model.FirstName
    </dd>
    <dt class="col-sm-2">
        LastName:
    </dt>
    <dd class="col-sm-10">
        @Model.LastName
    </dd>
    <dt class="col-sm-2">
        Email:
    </dt>
    <dd class="col-sm-10">
        @Model.Email
    </dd>
    <dt class="col-sm-2">
        Phone:
    </dt>
    <dd class="col-sm-10">
        @Model.PhoneNumber
    </dd>
    <dt class="col-sm-2">
        Organization:
    </dt>
    <dd class="col-sm-10">
        @Model.Organization
    </dd>
    <dt class="col-sm-2">
        Position:
    </dt>
    <dd class="col-sm-10">
        @Model.Position
    </dd>

    <dt class="col-sm-2">
        Address:
    </dt>
    <dd class="col-sm-10">
        @Model.Address
    </dd>
</dl> 