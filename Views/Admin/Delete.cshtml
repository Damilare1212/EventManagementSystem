@{
    Layout = "_AdminDashBoard";
}
@model EventApp.DTOs.AdminDto



<h3>Are you sure you want to delete this Admin?</h3>
<div style="font-size: 130%;">
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            First Name:
        </dt>
        <dd class="col-sm-10">
            @Model.FirstName
        </dd>
        <dt class="col-sm-2">
            Last Name:
        </dt>
        <dd class="col-sm-10">
            @Model.LastName
        </dd>

    </dl>

    <form asp-action="Delete">
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>